+                          nh++;
+		       } else {
+			  free(newword);
+                       }
+	            }
+                }
+                cptr = (PfxEntry *)cptr ->getFlgNxt();
+             }
+	  }
+       }
+
+
+    // now handle pure prefixes
+    for (int m = 0; m < al; m ++) {
