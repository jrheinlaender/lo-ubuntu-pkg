-Procedure GID_PROCEDURE_COPY_OPL_RES
-    ProcName = "CopyOplRes";
-    Styles = (STANDALONE, NETWORK, INSTALL, AFTER_INST);
-    Code = {Sub  CopyOplRes
-    rem is, 11.05.2001
-    rem Renames the opl-res into sop-res for OpenOffice Player
-    dim resourcePath As String
-    dim intVclNr as integer
-    dim i as integer
-    dim countrynumber as integer
-    dim aFileName as String
-    dim quelldatei as String
-    dim zieldatei as String
-    dim isoresource as String
-
-    countrynumber = 13
-
-    dim countrystrings(countrynumber) as String
-
-    countrystrings(0) = "01"
-    countrystrings(1) = "03"
-    countrystrings(2) = "07"
-    countrystrings(3) = "31"
-    countrystrings(4) = "33"
-    countrystrings(5) = "34"
-    countrystrings(6) = "39"
-    countrystrings(7) = "45"
-    countrystrings(8) = "46"
-    countrystrings(9) = "48"
-    countrystrings(10) = "49"
-    countrystrings(11) = "81"
-    countrystrings(12) = "86"
-    countrystrings(13) = "88"
-
-    resourcePath = DirEntry( GetSetupEnv.DestPath, "program" )
-	intVclNr = GetMajor()
-
-    for i=0 to countrynumber
-
-    isoresource = "opl" & intVclNr & countrystrings(i) & ".res"
-    aFileName = Dir(resourcePath,0)
-       Do while (aFileName <> "")
-         If (aFileName = isoresource) Then
-            quelldatei = DirEntry(resourcePath, isoresource)
-            zieldatei = DirEntry(resourcePath, "sop" & intVclNr & countrystrings(i) & ".res")
-            FileCopy (quelldatei, zieldatei)
-         End If
-         aFileName = Dir
-       Loop
-
-    next i
-
-    End Sub
-};
-End
-
-#endif
-
-#ifdef UNX
-
-Procedure GID_PROCEDURE_CDE_DTINT
-        ProcName     = "CDEDtint";
