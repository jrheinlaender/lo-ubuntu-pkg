+// sort in decreasing order of score
+void SuggestMgr::bubblesort(char** rword, int* rsc, int n )
+{
+      int m = 1;
+      while (m < n) {
+	  int j = m;
+	  while (j > 0) {
+	    if (rsc[j-1] < rsc[j]) {
+	        int sctmp = rsc[j-1];
+                char * wdtmp = rword[j-1];
+	        rsc[j-1] = rsc[j];
+                rword[j-1] = rword[j];
+                rsc[j] = sctmp;
+                rword[j] = wdtmp;
+	        j--;
+	    } else break;
+	  }
+          m++;
+      }
+      return;
 }
