+Procedure GID_PROCEDURE_PYTHON_INSTALL
+    ProcName = "PythonInst";
+    Styles = (STANDALONE,NETWORK,INSTALL,AFTER_INST, REPAIRABLE);
+    ModuleID = GID_MODULE_PYUNO;
+    Code = {Sub PythonInst
+            On Error goto PYTHON_END
+	fileHandle = FreeFile
+	open curdir()+"\python.bat" for Output Access Write as fileHandle
+	print #filehandle, "@echo off"
+	print #filehandle, "set PATH=%PATH%;"+curdir()
+	print #fileHandle, "set PYTHONPATH="+curdir()+";"+curdir()+"\python-core-2.2.2\lib"
+	print #fileHandle, "set PYTHONHOME="+curdir()+"\python-core-2.2.2"
+	print #fileHandle, curdir()+"\python-core-2.2.2\bin\python.exe %1 %2 %3 %4 %5 %6 %7 %8 %9"
+	close #fileHandle
+
+	PYTHON_END:
+	End Sub
+	};
+End
