+// generate an n-gram score comparing s1 and s2
+int SuggestMgr::ngram(int n, char * s1, const char * s2, bool uselen)
+{
+  int nscore = 0;
+  int l1 = strlen(s1);
+  int l2 = l1;
+  if (uselen) l2 = strlen(s2);
+  int ns;
+  for (int j=1;j<=n;j++) {
+    ns = 0;
+    for (int i=0;i<=(l1-j);i++) {
+      char c = *(s1 + i + j);
+      *(s1 + i + j) = '\0';
+      if (strstr(s2,(s1+i))) ns++;
+      *(s1 + i + j ) = c;
+    }
+    nscore = nscore + ns;
+    if (ns < 2) break;
+  }
+  ns = abs(l1-l2) - 2;
+  return (nscore - ((ns > 0) ? ns : 0));
+}
