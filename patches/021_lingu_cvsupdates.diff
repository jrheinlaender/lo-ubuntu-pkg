+void enmkallcap(char * d, const char * p, const char * encoding)
+{
+  struct cs_info * csconv = get_current_cs(encoding);
+  while (*p != '\0') { 
+    *d++ = csconv[((unsigned char) *p)].cupper;
+    p++;
+  }
+  *d = '\0';
+}
