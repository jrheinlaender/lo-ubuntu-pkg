++void Desktop::ensureProcessServiceFactory()
+ {
++    if( ::comphelper::getProcessServiceFactory().is())
++        return;
++
+     Reference < XMultiServiceFactory > rSMgr = CreateApplicationServiceManager();
+     if( rSMgr.is() )
+     {
