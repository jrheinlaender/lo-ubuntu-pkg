+ 
+-SwViewOptionAdjust_Impl::SwViewOptionAdjust_Impl( ViewShell& rSh, const SwViewOption &rViewOptions, bool bIsTmpSelection ) :
+-    m_rShell( rSh ),
+-    m_aOldViewOptions( rViewOptions ),
