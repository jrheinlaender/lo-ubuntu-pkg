+-void XFileStream::fill( sal_Int64 nUntil)
+-{
+-    sal_Int32 nRead;
+-    sal_Int64 nPosition = mxTempSeek->getPosition();
+-    mxTempSeek->seek ( mxTempSeek->getLength() );
+-    maBuffer.realloc ( n_ConstBufferSize );
