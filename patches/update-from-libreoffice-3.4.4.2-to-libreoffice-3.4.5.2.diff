+-        nBytesToRead = static_cast < sal_Int32 > ( mnZipSize - nPosition );
+-
+-    sal_Int64 nUntil = nBytesToRead + nPosition + n_ConstBufferSize;
+-    if (nUntil > mnZipSize )
+-        nUntil = mnZipSize;
+-    if ( nUntil > mxTempSeek->getLength() )
+-        fill ( nUntil );
+-    sal_Int32 nRead = mxTempIn->readBytes ( aData, nBytesToRead );
+-    return nRead;
+-}
